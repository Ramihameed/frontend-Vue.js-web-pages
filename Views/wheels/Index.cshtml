@model List<wheels>

@{
    var searchId = ViewData["searchId"] as int?;
}

<h1>Edit Wheel</h1>

<form method="get" action="@Url.Action("Edit", "Wheels")">
    <div class="form-group">
        <label for="searchId">Enter Wheel ID to Edit</label>
        <input type="number" id="searchId" name="id" class="form-control" required />
    </div>

    <button type="submit" class="btn btn-secondary">Search</button>
</form>


